4th Challenge

fun main(){

val string="{{{}}}"
    print( countC(string,'{',']')

}

fun countC(string: String,char1: Char,char2: Char):String{
  val a=  string.count { it==char1 }
    val b= string.count { it==char2 }

    return if (a==b){
        return "true"
    }else{
        "false"
    }
}